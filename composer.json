{
    "name": "SIIG/eTAB",
    "description": "SIIG/eTAB",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.3",
                
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/polyfill-apcu": "^1.0",
        "twig/extensions": "v1.3.0",
        "symfony/assetic-bundle": "v2.8.0",
        "kriswallsmith/assetic": "v1.3.2",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sensio/generator-bundle": "v3.0.7",
        "incenteev/composer-parameter-handler": "^2.0",
        
        "knplabs/knp-menu-bundle":"dev-master",
        
        "sonata-project/cache": "v1.0.7",
        "sonata-project/cache-bundle": "dev-master",
        "sonata-project/datagrid-bundle": "^2.2",
        "sonata-project/core-bundle": "3.*",
        "sonata-project/admin-bundle": "3.*",
        "sonata-project/doctrine-orm-admin-bundle": "3.*",
        "sonata-project/block-bundle": "3.*",
        "sonata-project/easy-extends-bundle": "2.x-dev",
        "sonata-project/user-bundle": "dev-master",
        
        	
        "simplethings/entity-audit-bundle": "v0.6",
	"friendsofsymfony/user-bundle": "v1.3.6",
        "friendsofsymfony/jsrouting-bundle": "v1.6.0",
        
        "os/excel-bundle": "v2.1",
        "php-amqplib/php-amqplib": "~2.6",
        "php-amqplib/rabbitmq-bundle": "^1.10",
        "fr3d/ldap-bundle": "v3.0.0",
	"knplabs/knp-snappy-bundle": "v1.4",
                
        "suncat/mobile-detect-bundle": "v1.0.3",
        "jms/serializer-bundle": "v1.1.0",
        "friendsofsymfony/rest-bundle": "^1.1",
        "snc/redis-bundle": "v2.0.0",
        "predis/predis": "v1.1.1",
        "h4cc/wkhtmlpdf-amd64": "0.12.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-tests-dir": "tests",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "symfony-assets-install": "symlink"
    }
}
